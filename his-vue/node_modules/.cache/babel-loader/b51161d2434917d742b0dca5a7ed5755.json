A,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,EAAA,UAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,EAEA;AAAA,UAAA,GAAA,EAAA,CAAA;AAAA,UAAA,GAAA,EAAA,GAAA;AAAA,UAAA,OAAA,EAAA,iBAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAFA,EAGA;AAAA,UAAA,SAAA,EAAA,SAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAHA;AADA;AAPA,KAAA;AAeA,GA/BA;AAgCA,EAAA,OAhCA,qBAgCA;AAAA;;AACA,QAAA,KAAA,MAAA,EAAA;AACA;AACA,WAAA,MAAA,CAAA,GAAA,wBAAA,KAAA,MAAA,GAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,OALA;AAMA;AACA,GA1CA;AA2CA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AAAA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,CAAA,KAAA,EAAA;;AACA,YAAA,MAAA,CAAA,MAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,GAAA,wBAAA,MAAA,CAAA,MAAA,GAAA,UAAA,QAAA,EAAA;AACA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,aAAA,EAAA,KAAA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,WAJA,EAIA,MAAA,CAAA,IAJA;AAKA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,cAAA,EAAA,UAAA,QAAA,EAAA;AACA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,aAAA,EAAA,KAAA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,WAJA,EAIA,MAAA,CAAA,IAJA;AAKA;AACA,OAfA;AAgBA;AAlBA;AA3CA,CAAA","sourcesContent":["<template>\n  <div>\n    <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"120px\">\n      <el-form-item label=\"科室名称\" prop=\"name\">\n        <el-input v-model=\"form.name\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"办公地址\" prop=\"address\">\n        <el-input v-model=\"form.address\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"负责人\" prop=\"leader\">\n        <el-input v-model=\"form.leader\"></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" plain @click=\"save\">保存数据</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'DepartmentEdit',\n  props: ['editid'],\n  data () {\n    var checkName = (rule, value, cb) => {\n      if (this.editid) cb()\n      const promise = this.$axios.http.get('/departments/check', { params: { name: value } })\n      promise.then(\n        response => {\n          if (response.data.status !== 200) {\n            cb(new Error('该科室名称已存在'))\n          }\n          cb()\n        }\n      )\n    }\n    return {\n      options: [],\n      form: {\n        name: '',\n        address: '',\n        leader: ''\n      },\n      rules: {\n        name: [\n          { required: true, message: '科室名称不能为空', trigger: 'blur' },\n          { min: 1, max: 100, message: '长度在 1 到 100 个字符', trigger: 'blur' },\n          { validator: checkName, trigger: 'blur' }\n        ]\n      }\n    }\n  },\n  created () {\n    if (this.editid) {\n      // 通过id 读原始数据\n      this.$axios.get(`/departments/${this.editid}`, response => {\n        this.form.name = response.data.name\n        this.form.address = response.data.address\n        this.form.leader = response.data.leader\n        this.form.type = response.data.type\n      })\n    }\n  },\n  methods: {\n    save () {\n      this.$refs.form.validate(valid => {\n        if (!valid) return\n        if (this.editid) {\n          this.$axios.put(`/departments/${this.editid}`, response => {\n            // 子组件要改变父组件的值\n            this.$emit('update:show', false)\n            this.$emit('getData')\n          }, this.form)\n        } else {\n          this.$axios.post('/departments', response => {\n            // 子组件要改变父组件的值\n            this.$emit('update:show', false)\n            this.$emit('getData')\n          }, this.form)\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/components/department"}]}