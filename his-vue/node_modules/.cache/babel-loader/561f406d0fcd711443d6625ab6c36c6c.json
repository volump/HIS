;AACA,OAJA,EAKA,KAAA,SALA;AAOA,KATA;AAUA,IAAA,GAVA,eAUA,IAVA,EAUA;AACA,WAAA,MAAA,CAAA,IAAA,YAAA,KAAA,GAAA,aACA,YAAA,CAAA,CADA,EAEA;AACA,QAAA,UAAA,EAAA,KAAA,MADA;AAEA,QAAA,MAAA,EAAA,IAAA,CAAA,EAFA;AAGA,QAAA,GAAA,EAAA,IAAA,CAAA,GAHA;AAIA,QAAA,QAAA,EAAA,IAAA,CAAA,IAJA;AAKA,QAAA,MAAA,EAAA;AALA,OAFA;AAUA,KArBA;AAsBA,IAAA,gBAtBA,4BAsBA,OAtBA,EAsBA;AACA,WAAA,SAAA,CAAA,KAAA,GAAA,OAAA;AACA,WAAA,OAAA;AACA,KAzBA;AA0BA;AACA,IAAA,mBA3BA,+BA2BA,OA3BA,EA2BA;AACA,WAAA,SAAA,CAAA,IAAA,GAAA,OAAA;AACA,WAAA,OAAA;AACA;AA9BA;AA5BA,CAAA","sourcesContent":["<template>\n  <div>\n    <el-card>\n      <el-row :gutter=\"10\">\n        <el-col :span=\"10\">\n          <el-input v-model=\"queryInfo.name\" :placeholder=\"'请输入要查的'+title+'项目名称'\" clearable @clear=\"getData\">\n            <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"getData\"></el-button>\n          </el-input>\n        </el-col>\n      </el-row>\n\n      <el-table\n        :data=\"tableData\"\n        style=\"width: 100%\"\n      >\n        <el-table-column label=\"ID\" width=\"80\" prop=\"id\" ></el-table-column>\n        <el-table-column :label=\"title+'名称'\" prop=\"name\"></el-table-column>\n        <el-table-column :label=\"title+'内容'\" prop=\"content\"></el-table-column>\n        <el-table-column :label=\"title+'费用'\" prop=\"fee\" width=\"80px\"></el-table-column>\n        <el-table-column label=\"操作\" width=\"220px\">\n          <template slot-scope=\"scope\">\n              <el-tooltip effect=\"dark\" content=\"添加\" placement=\"top\" :enterable=\"false\">\n                <el-button type=\"success\" icon=\"el-icon-check\" circle @click=\"add(scope.row)\"></el-button>\n              </el-tooltip>\n          </template>\n        </el-table-column>\n      </el-table>\n      <!-- 分页区域 -->\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"queryInfo.page\"\n        :page-sizes=\"[3, 5, 10, 20]\"\n        :page-size=\"queryInfo.limit\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n      ></el-pagination>\n    </el-card>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: ['title', 'caseId', 'url'],\n  data () {\n    return {\n      queryInfo: {\n        name: '',\n        page: 1,\n        limit: 5,\n        active: 1\n      },\n      tableData: [],\n      total: 0,\n      selectedrow: []\n    }\n  },\n  mounted () {\n    if (this.url) {\n      this.getData()\n    }\n  },\n  watch: {\n    // 正确给 url 赋值的 方法\n    url: function (newVal, oldVal) {\n      this.url = newVal // newVal即是url\n      console.log(this.url)\n      this.getData()\n    }\n  },\n  methods: {\n    getData () {\n      this.$axios.get(`/${this.url}Items`,\n        response => {\n          this.tableData = response.data.records\n          this.total = parseInt(response.data.total)\n        },\n        this.queryInfo\n      )\n    },\n    add (item) {\n      this.$axios.post(`/${this.url}Applys`,\n        () => {},\n        {\n          registerId: this.caseId,\n          itemId: item.id,\n          fee: item.fee,\n          itemName: item.name,\n          status: 1\n        }\n      )\n    },\n    handleSizeChange (newSize) {\n      this.queryInfo.limit = newSize\n      this.getData()\n    },\n    // 监听 页码值 改变的事件\n    handleCurrentChange (newPage) {\n      this.queryInfo.page = newPage\n      this.getData()\n    }\n  }\n}\n</script>\n\n<style lang='less' scoped>\n</style>\n"],"sourceRoot":"src/components/doctor"}]}