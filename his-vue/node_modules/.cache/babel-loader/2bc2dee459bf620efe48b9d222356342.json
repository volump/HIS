A,GAxBA;AAyBA,EAAA,OAzBA,qBAyBA;AAAA;;AACA,QAAA,KAAA,MAAA,EAAA;AACA;AACA,WAAA,MAAA,CAAA,GAAA,uBAAA,KAAA,MAAA,GAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,GAAA,GAAA,QAAA,CAAA,IAAA,CAAA,GAAA;AACA,OAJA;AAKA;AACA,GAlCA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AAAA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,CAAA,KAAA,EAAA;;AACA,YAAA,MAAA,CAAA,MAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,GAAA,uBAAA,MAAA,CAAA,MAAA,GAAA,UAAA,QAAA,EAAA;AACA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,aAAA,EAAA,KAAA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,WAJA,EAIA,MAAA,CAAA,IAJA;AAKA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,aAAA,EAAA,UAAA,QAAA,EAAA;AACA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,aAAA,EAAA,KAAA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,WAJA,EAIA,MAAA,CAAA,IAJA;AAKA;AACA,OAfA;AAgBA;AAlBA;AAnCA,CAAA","sourcesContent":["<template>\n  <div>\n    <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"120px\">\n      <el-form-item label=\"检查项目名称\"  prop=\"name\">\n        <el-input v-model=\"form.name\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"检查内容\"  prop=\"name\">\n        <el-input v-model=\"form.content\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"费用单价\"  prop=\"fee\">\n        <el-input v-model=\"form.fee\" ></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" plain @click=\"save\">保存数据</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'CheckItemEdit',\n  props: ['editid'],\n  data () {\n    return {\n      options: [],\n      form: {\n        name: '',\n        fee: '',\n        content: ''\n      },\n      rules: {\n        fee: [\n          { required: true, message: '费用不能为空', trigger: 'blur' }\n        ],\n        name: [\n          { required: true, message: '不能为空', trigger: 'blur' },\n          { min: 1, max: 100, message: '长度在 1 到 100 个字符', trigger: 'blur' }\n        ],\n        content: [\n          { required: true, message: '不能为空', trigger: 'blur' }\n        ]\n      }\n    }\n  },\n  created () {\n    if (this.editid) {\n      // 通过id 读原始数据\n      this.$axios.get(`/checkItems/${this.editid}`, response => {\n        this.form.content = response.data.content\n        this.form.name = response.data.name\n        this.form.fee = response.data.fee\n      })\n    }\n  },\n  methods: {\n    save () {\n      this.$refs.form.validate(valid => {\n        if (!valid) return\n        if (this.editid) {\n          this.$axios.put(`/checkItems/${this.editid}`, response => {\n            // 子组件要改变父组件的值\n            this.$emit('update:show', false)\n            this.$emit('getData')\n          }, this.form)\n        } else {\n          this.$axios.post('/checkItems', response => {\n            // 子组件要改变父组件的值\n            this.$emit('update:show', false)\n            this.$emit('getData')\n          }, this.form)\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/components/check-item"}]}